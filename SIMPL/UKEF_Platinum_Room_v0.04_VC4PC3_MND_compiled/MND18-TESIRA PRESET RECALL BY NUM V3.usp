_SKIP_,_SKIP_,
PRESET_NAME$[30][50];

STRING_OUTPUT TX$;



/****************************************************************************
  VARIABLE DEFINITIONS GO BELOW  
  EG: INTEGER // STRING 
*****************************************************************************/   
INTEGER LAST;
/****************************************************************************
  SUBROUTINE & FUNCTION DEFINITIONS GO BELOW
*****************************************************************************/   


/*******************************************************************************************
  THE EVENTS GO BELOW 
  EG: PUSH, RELEASE,CHANGE 
*******************************************************************************************/
PUSH PRESET_GO
{
LAST=GETLASTMODIFIEDARRAYINDEX();
TX$="DEVICE recallPreset "+PRESET_NAME$[GETLASTMODIFIEDARRAYINDEX()]+"\x0A";
}

PUSH call_again
{
IF(LAST) TX$="DEVICE recallPresetByName \x22"+PRESET_NAME$[LAST]+"\x22\x0A";
}
     
