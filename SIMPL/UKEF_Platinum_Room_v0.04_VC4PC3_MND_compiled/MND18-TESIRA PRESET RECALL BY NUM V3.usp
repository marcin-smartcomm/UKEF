      DIGITAL_OUTPUT // ANALOG_OUTPUT // STRING_OUTPUT
*****************************************************************************/   
DIGITAL_INPUT

call_again,PRESET_GO[30];

STRING_PARAMETER 
_SKIP_,_SKIP_,
_SKIP_,_SKIP_,
_SKIP_,_SKIP_,
_SKIP_,_SKIP_,
_SKIP_,_SKIP_,
_SKIP_,_SKIP_,
